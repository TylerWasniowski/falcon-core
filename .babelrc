{
  "presets": [["env", {
    "targets": {
      "node": 6
    }
  }]],
  "env": {
    "development": {
      "plugins": [
        ["flow-runtime", {"assert": true,"annotate": true}]
      ]
    },
    "test": {
      "plugins": [
        ["flow-runtime", {"assert": true,"annotate": true}]
      ]
    }
  },
  "plugins": [
    "syntax-flow",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-runtime",
    "add-module-exports",
    "transform-flow-strip-types"
  ]
}
